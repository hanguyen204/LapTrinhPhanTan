<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<AnchorPane xmlns="http://javafx.com/javafx" fx:id="ap"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.javafx.week2.TH.chooser_Controller"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <ImageView fx:id="imageView" fitWidth="396" fitHeight="224" layoutX="100" layoutY="50" pickOnBounds="true"
                   preserveRatio="true"/>
        <Button layoutX="256" layoutY="300" mnemonicParsing="false" onAction="#chooseImage" text="Open"/>
    </children>
</AnchorPane>
